pfit_function <- function(dat, index){
int <- summary(glm(death30 ~ proced + asa + bmi, data = dat[index,],
family = binomial(link = "logit"))$coefficients[1, 1])
pred_proced <- summary(glm(death30 ~ proced + asa + bmi, data = dat[index,],
family = binomial(link = "logit"))$coefficients[2, 1])
pred_asa <- summary(glm(death30 ~ proced + asa + bmi, data = dat[index,],
family = binomial(link = "logit"))$coefficients[3, 1])
pred_bmi <- summary(glm(death30 ~ proced + asa + bmi, data = dat[index,],
family = binomial(link = "logit"))$coefficients[4, 1])
XB <- int + pred_proced*dat$proced + pred_asa*dat$asa + pred_bmi*dat$bmi
p_fit <- inv.logit(XB)
}
